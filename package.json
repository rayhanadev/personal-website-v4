		"koa-static": "^5.0.0",
		"koa2-cors": "^2.0.6",
		"lru-cache": "^7.14.1",
		"nanostores": "^0.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.42.1",
		"sharp": "^0.31.3",
		"swr": "^2.0.3",
		"tailwindcss": "^3.2.4",
		"unplugin-auto-import": "^0.13.0",
